{
  "manifest_version": 2,
  "name": "message",
  "version": "1.0",
  "background": {
    "scripts": ["background/background.js"],
    "persistent": true
  },
  "minimum_chrome_version":"36",
  "chrome_url_overrides" : {
    "newtab": "newtab/newtab.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.baidu.com/*"
      ],
      "js": [
        "content/content.js"
      ]
    }
  ],
  "permissions": [
  ]
}